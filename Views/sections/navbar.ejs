<style>
    .yellow-nav {
      background-color: #ffd700 !important;
      padding: 7px;
    }
    .nav-link {
      font-size: 16px !important;
      color: #143556 !important;
      font-weight: 500 !important;
    }
    .dropdown-item {
      color: black !important;
    }
    .dropdown-item:hover {
      color: gray !important;
    }
  
    body {
      background-color: #ffffff !important;
    }
  
    .logo-visions {
      width: 60px;
      height: 40px;
    }
  </style>
  <% if (typeof serviceId != undefined && typeof serviceName != undefined) {%>
    <div class="navbar navbar-expand-lg navbar-light yellow-nav">
      <div class="container px-4">
        <div class="collapse navbar-collapse" id="mobile-nav">
          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <a class="nav-link" href="/webApp/purposes">Traitements</a>
            </li>
            <li class="nav-item">
              <a class="nav-link">
                <%= serviceName %>
              </a>
            </li>
          </ul>
        </div>
      </div>
    </div>
  <% } else { %>
    <div class="navbar navbar-expand-lg navbar-light yellow-nav">
      <div class="container px-4">
        <a href="/webApp" class="navbar-brand" rel="home" title="Visions">
          <img
            src="/images/visions-logo.svg"
            alt="Visions Logo"
            class="logo-visions"
          />
        </a>
      </div>
    </div>
  <% } %>
    
  